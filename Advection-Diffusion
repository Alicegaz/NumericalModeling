    "f2.write(str(M)+\" \")\n",
    "it = 0\n",
    "for c, u in zip(coords, x):\n",
    "    f2.write(str(c[0]) + \" \" + str(c[1]) + \" \" + str(u)+\" \")\n",
    "    if ((it % 4*M) == 0) and (it > 0):\n",
